      object daSQL2: TdaSQL
        DatabaseName = 'MyCnx'
        DatabaseType = dtPostgreSQL
        DataPipelineName = 'saldo1'
        EditSQLAsText = True
        IsCaseSensitive = True
        LinkColor = clNavy
        LinkType = ltParameterizedSQL
        MaxSQLFieldAliasLength = 0
        SQLText.Strings = (
          ' SELECT SUM(TOTALDEBITO) AS TOTALDEBITO,  '
          '        SUM(TOTALCREDITO) AS TOTALCREDITO '
          ' FROM ( '
          ' SELECT '
          '   COALESCE(SUM(R.VALOR), 0) AS TOTALDEBITO, '
          '   0 AS TOTALCREDITO '
          '  FROM financas.LANCAMENTOSCONTAS L '
          '   JOIN financas.CONTAS C ON C.CONTA = L.CONTA'
          
            '   JOIN financas.DOCUMENTOSRATEADOS D ON D.DOCUMENTORATEADO = L.' +
            'DOCUMENTORATEADO '
          
            '   JOIN financas.RATEIOSFINANCEIROS R ON R.DOCUMENTORATEADO = D.' +
            'DOCUMENTORATEADO '
          
            '   JOIN financas.CENTROSCUSTOS CC ON CC.CENTROCUSTO = R.CENTROCU' +
            'STO '
          '    WHERE '
          '     CC.CENTROCUSTO IN :CentrosCustos    '
          '     AND CASE :TipoData'
          '       WHEN 0 THEN (L.DATA BETWEEN :DataInicial AND :DataFinal)'
          
            '       WHEN 1 THEN (L.DATACOMPETENCIA BETWEEN :DataInicial AND :' +
            'DataFinal)'
          '     END '
          
            '     AND (CASE WHEN :FiltrarEstab THEN (C.ESTABELECIMENTO = :Est' +
            'abelecimentos) ELSE TRUE END)'
          '     AND (SINAL = 0) '
          '     AND (CC.CENTROCUSTO NOT IN ( SELECT C1.PAIID '
          
            '                                  FROM financas.CENTROSCUSTOS C1' +
            ' WHERE C1.PAIID = CC.CENTROCUSTO ))'
          '     AND (CC.SITUACAO = 0) '
          '  UNION ALL '
          '  SELECT '
          '   0 AS TOTALDEBITO, '
          '   COALESCE(SUM(R.VALOR), 0) AS TOTALCREDITO '
          '  FROM financas.LANCAMENTOSCONTAS L '
          '   JOIN financas.CONTAS C ON C.CONTA = L.CONTA'
          
            '   JOIN financas.DOCUMENTOSRATEADOS D ON D.DOCUMENTORATEADO = L.' +
            'DOCUMENTORATEADO '
          
            '   JOIN financas.RATEIOSFINANCEIROS R ON R.DOCUMENTORATEADO = D.' +
            'DOCUMENTORATEADO '
          
            '   JOIN financas.CENTROSCUSTOS CC ON CC.CENTROCUSTO = R.CENTROCU' +
            'STO '
          '   WHERE '
          '     CC.CENTROCUSTO IN :CentrosCustos '
          '     AND CASE :TipoData'
          
            '           WHEN 0 THEN (L.DATA BETWEEN :DataInicial AND :DataFin' +
            'al)'
          
            '           WHEN 1 THEN (L.DATACOMPETENCIA BETWEEN :DataInicial A' +
            'ND :DataFinal)'
          '         END '
          
            '     AND (CASE WHEN :FiltrarEstab THEN (C.ESTABELECIMENTO = :Est' +
            'abelecimentos) ELSE TRUE END)    '
          '     AND (SINAL = 1) '
          '     AND (CC.CENTROCUSTO NOT IN ( SELECT C1.PAIID '
          
            '                                  FROM financas.CENTROSCUSTOS C1' +
            ' WHERE C1.PAIID = CC.CENTROCUSTO ))'
          '     AND (CC.SITUACAO = 0) '
          '  UNION ALL '
          'SELECT '
          'COALESCE(SUM(L.VALOR), 0) AS TOTALDEBITO, '
          '0 AS TOTALCREDITO '
          '  FROM financas.LANCAMENTOSCONTAS L '
          '  JOIN financas.CONTAS C ON C.CONTA = L.CONTA'
          
            '  LEFT JOIN FINANCAS.RATEIOSFINANCEIROS R ON R.DOCUMENTORATEADO ' +
            '= L.DOCUMENTORATEADO'
          '  WHERE '
          '    CASE :TipoData'
          '      WHEN 0 THEN (L.DATA BETWEEN :DataInicial AND :DataFinal)'
          
            '      WHEN 1 THEN (L.DATACOMPETENCIA BETWEEN :DataInicial AND :D' +
            'ataFinal)'
          '    END '
          
            '    AND (CASE WHEN :FiltrarEstab THEN (C.ESTABELECIMENTO = :Esta' +
            'belecimentos) ELSE TRUE END)'
          '    AND (SINAL = 0) '
          
            '    AND ((L.DOCUMENTORATEADO IS NULL) OR (R.CENTROCUSTO IS NULL)' +
            ') '
          '  UNION ALL '
          'SELECT '
          '0 AS TOTALDEBITO, '
          'COALESCE(SUM(L.VALOR), 0) AS TOTALCREDITO '
          '  FROM financas.LANCAMENTOSCONTAS L '
          '  JOIN financas.CONTAS C ON C.CONTA = L.CONTA'
          
            '  LEFT JOIN FINANCAS.RATEIOSFINANCEIROS R ON R.DOCUMENTORATEADO ' +
            '= L.DOCUMENTORATEADO'
          '  WHERE '
          '   CASE :TipoData'
          '     WHEN 0 THEN (L.DATA BETWEEN :DataInicial AND :DataFinal)'
          
            '     WHEN 1 THEN (L.DATACOMPETENCIA BETWEEN :DataInicial AND :Da' +
            'taFinal)'
          '   END '
          
            '   AND (CASE WHEN :FiltrarEstab THEN (C.ESTABELECIMENTO = :Estab' +
            'elecimentos) ELSE TRUE END)'
          '   AND (SINAL = 1) '
          
            '   AND ((L.DOCUMENTORATEADO IS NULL) OR (R.CENTROCUSTO IS NULL))' +
            '  '
          ' ) AS SALDO '
          '')
        SQLType = sqSQL2
