      object daSQL1: TdaSQL
        DatabaseName = 'MyCnx'
        DatabaseType = dtPostgreSQL
        DataPipelineName = 'dados'
        EditSQLAsText = True
        IsCaseSensitive = True
        LinkColor = clMaroon
        LinkType = ltParameterizedSQL
        MaxSQLFieldAliasLength = 0
        SQLText.Strings = (
          'SELECT '
          ' CC.CENTROCUSTO, '
          ' COALESCE(CC.CODIGO, '#39'000'#39') AS CENTROCUSTOCODIGO,'
          
            ' COALESCE(CC.DESCRICAO, '#39'Sem Centro de Custo'#39') AS CENTROCUSTONOM' +
            'E,'
          ' CF.CLASSIFICACAOFINANCEIRA, '
          ' CF.CODIGO AS CLASSIFICACAOCODIGO, '
          ' CF.DESCRICAO AS CLASSIFICACAONOME, '
          ' L.LANCAMENTOCONTA, '
          ' CASE :TipoData'
          '   WHEN 0 THEN L.DATA'
          '   WHEN 1 THEN L.DATACOMPETENCIA'
          ' END AS LANCAMENTODATA, '
          ' L.CONTA, '
          ' C.CODIGO AS CONTACODIGO,'
          ' L.DOCUMENTO, '
          ' L.HISTORICO, '
          ' L.SINAL AS SINALLANCAMENTO, '
          ' CASE L.SINAL'
          '   WHEN 0 THEN '#39'D'#233'bito'#39
          '   WHEN 1 THEN '#39'Cr'#233'dito'#39'  '
          ' END AS SINALLANCAMENTOTXT, '
          ' COALESCE(R.VALOR, L.VALOR) AS RATEIOVALOR,'
          ' CC.NATUREZA,'
          ' CASE CC.NATUREZA'
          '   WHEN 0 THEN '#39'Indefinido'#39
          '   WHEN 1 THEN '#39'Lucro'#39
          '   WHEN 2 THEN '#39'Custo'#39'  '
          ' END AS NATUREZATXT,'
          ' L.FAVORECIDO,'
          
            ' (select origemsintetica from financas.fu_getorigensinteticocent' +
            'rocusto(null,true,CC.CENTROCUSTO) limit 1) as ORIGEMCENTROCUSTO ' +
            '    '
          ' FROM financas.LANCAMENTOSCONTAS L '
          ' JOIN financas.CONTAS C ON C.CONTA = L.CONTA'
          
            ' LEFT JOIN financas.DOCUMENTOSRATEADOS D ON D.DOCUMENTORATEADO =' +
            ' L.DOCUMENTORATEADO '
          
            ' LEFT JOIN financas.RATEIOSFINANCEIROS R ON R.DOCUMENTORATEADO =' +
            ' D.DOCUMENTORATEADO                        '
          
            ' LEFT JOIN financas.CENTROSCUSTOS CC ON CC.CENTROCUSTO = R.CENTR' +
            'OCUSTO '
          
            ' LEFT JOIN financas.CLASSIFICACOESFINANCEIRAS CF ON CF.CLASSIFIC' +
            'ACAOFINANCEIRA = R.CLASSIFICACAOFINANCEIRA'
          ' WHERE'
          
            ' (CASE WHEN :IncluirNulo THEN ((CC.CENTROCUSTO IN :CentrosCustos' +
            ') or (CC.CENTROCUSTO IS NULL)) ELSE (CC.CENTROCUSTO IN :CentrosC' +
            'ustos) END) '
          ' AND CASE :TipoData'
          '       WHEN 0 THEN (L.DATA BETWEEN :DataInicial AND :DataFinal)'
          
            '       WHEN 1 THEN (L.DATACOMPETENCIA BETWEEN :DataInicial AND :' +
            'DataFinal)'
          '     END  '
          ' AND (CC.CENTROCUSTO NOT IN ( SELECT C1.PAIID '
          
            '      FROM financas.CENTROSCUSTOS C1 WHERE C1.PAIID = CC.CENTROC' +
            'USTO ) )'
          ' AND ((CC.SITUACAO = 0) OR (CC.SITUACAO IS NULL)) '
          
            ' AND (CASE WHEN :FiltrarEstab THEN (C.ESTABELECIMENTO = :Estabel' +
            'ecimentos) ELSE TRUE END)     '
          ' ORDER BY CASE :TipoOrdenacao '
          '            WHEN 0 THEN (CC.CODIGO, L.DATA)'
          '            WHEN 1 THEN (CC.CODIGO, CF.CODIGO, L.DATA)'
          '          END'
          '')
        SQLType = sqSQL2
